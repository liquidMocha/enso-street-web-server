{
  "defaultEnv": "local",
  "local": {
    "url": "postgres://enso-street:password@localhost:5432/enso-street"
  },
  "test": {
    "url": "DATABASE_URL",
    "ssl": true
  },
  "dev": {
    "url": "DATABASE_URL",
    "addIfNotExists": {
      "sslmode": "require"
    },
    "overwrite": {
      "native": true,
      "driver": "pg",
      "ssl": {
        "rejectUnauthorized": false
      }
    }
  }
}
