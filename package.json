{
  "name": "enso-street-web-server",
  "version": "0.0.0",
  "engines": {
    "node": "12.9.1"
  },
  "private": true,
  "scripts": {
    "start": "./node_modules/db-migrate/bin/db-migrate up -e local && node -r esm src/app.ts",
    "local": "./node_modules/db-migrate/bin/db-migrate up -e local --config ./src/database.json && node -r dotenv/config -r esm src/app.ts dotenv_config_path=./src/.env",
    "test": "tsc && DATABASE_URL=postgres://enso-street:password@localhost:5432/enso-street-test ./node_modules/db-migrate/bin/db-migrate up -e test && ALGOLIA_ITEM_INDEX=test-enso-street-items DATABASE_URL=postgres://enso-street:password@localhost:5432/enso-street-test REDIS_URL=redis://localhost:6379 sessionSecret=this-is-a-session-secret nyc ./node_modules/mocha/bin/mocha --exit -r esm",
    "tsc:w": "tsc -w",
    "build": "tsc",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.4",
    "@types/algoliasearch": "^3.34.10",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/hapi__joi": "^16.0.12",
    "@types/helmet": "0.0.45",
    "@types/lodash": "^4.14.149",
    "@types/morgan": "^1.9.0",
    "@types/redis": "^2.8.17",
    "algoliasearch": "^3.35.1",
    "aws-sdk": "^2.580.0",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "connect-redis": "^4.0.2",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.6",
    "db-migrate-pg": "^1.0.0",
    "debug": "~2.6.9",
    "deep-equal-in-any-order": "^1.0.21",
    "dotenv": "^8.1.0",
    "esm": "^3.2.25",
    "express": "~4.16.1",
    "express-session": "^1.17.0",
    "google-auth-library": "^5.2.2",
    "helmet": "^3.21.1",
    "http-errors": "~1.6.3",
    "lodash": "^4.17.15",
    "morgan": "~1.9.1",
    "newrelic": "^6.2.0",
    "pg-monitor": "^1.3.1",
    "pg-promise": "^10.3.1",
    "pug": "^2.0.4",
    "redis": "^2.8.0",
    "sinon": "^7.4.2",
    "uuid": "^3.3.3",
    "uuidv4": "^6.0.7"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/node": "^13.11.0",
    "chai": "^4.2.0",
    "chai-arrays": "^2.0.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "should": "^13.2.3",
    "source-map-support": "^0.5.17",
    "supertest": "^4.0.2",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  },
  "nyc": {
    "all": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "migrations/*",
      "test/*",
      "**/*.d.ts"
    ]
  }
}
